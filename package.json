{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "A simple and easy boilerplate for web developer.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weiyuxuan/react-boilerplate.git"
  },
  "author": "Wei Yuxuan",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "lint": "eslint --ext .js,jsx . --fix",
    "stylelint": "stylelint 'src/**/*.scss' --syntax=scss --fix",
    "cspell": "cspell 'src/**/*.{jsx,js,scss}'",
    "check-update": "npm-check -u"
  },
  "bugs": {
    "url": "https://github.com/weiyuxuan/react-boilerplate/issues"
  },
  "homepage": "https://github.com/weiyuxuan/react-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.2",
    "cspell": "^5.1.3",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.11.0",
